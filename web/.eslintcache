) => setShowOptions(showOptins ? false : true)}\r\n            >\r\n              <RSUserAvatar src={user?.avatar || \"\"} />\r\n              <RSOnOrOff\r\n                signed={!checked ? checked : signed}\r\n                position=\"absolute\"\r\n                bottom=\"0.1rem\"\r\n                right=\"0rem\"\r\n              />\r\n            </RSUserOptions>\r\n            <RSNotificationModal show={showNotifications}>\r\n              <RSNotificationHeader>\r\n                <div></div>\r\n                <RSNotificationTitle>Notifications</RSNotificationTitle>\r\n                <RiCloseLine\r\n                  size={20}\r\n                  color=\"#e0e0e0\"\r\n                  onClick={() => setShowNotifications(false)}\r\n                />\r\n              </RSNotificationHeader>\r\n\r\n              <RSNotificationBody>\r\n                <RSNotificationText>Most recent</RSNotificationText>\r\n                <RSNotificationsList>\r\n                  {notifications.map((item, index) => (\r\n                    <NotificationItem\r\n                      key={index}\r\n                      id={index}\r\n                      text={item}\r\n                      time=\"2 min\"\r\n                    />\r\n                  ))}\r\n                </RSNotificationsList>\r\n              </RSNotificationBody>\r\n            </RSNotificationModal>\r\n            <RSUserPanelModal show={showOptins}>\r\n              <RSUserPanelHeader>\r\n                <RSUserOptions>\r\n                  <RSUserAvatar src={user?.avatar || \"\"} />\r\n                  <RSOnOrOff\r\n                    signed={!checked ? checked : signed}\r\n                    position=\"absolute\"\r\n                    bottom=\"0.1rem\"\r\n                    right=\"0rem\"\r\n                  />\r\n                </RSUserOptions>\r\n                <RSUserName>{user?.name}</RSUserName>\r\n              </RSUserPanelHeader>\r\n              <RSUserPanelBody>\r\n              